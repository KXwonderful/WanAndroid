<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/customize_nav_graph"
    app:startDestination="@id/customizeFragment">
    <!-- customize 相关 fragment -->
    <fragment
        android:id="@+id/customizeFragment"
        android:name="com.wdf.myrobot.ui.fragment.customize.CustomizeFragment"
        android:label="fragment_customize"
        tools:layout="@layout/fragment_customize">
        <action
            android:id="@+id/action_to_cusNormalFragment"
            app:destination="@id/cusNormalFragment" />
        <action
            android:id="@+id/action_to_cusDimpleFragment"
            app:destination="@id/cusDimpleFragment" />
    </fragment>

    <fragment
        android:id="@+id/cusNormalFragment"
        android:name="com.wdf.myrobot.ui.fragment.customize.CusNormalFragment"
        android:label="fragment_cus_normal"
        tools:layout="@layout/fragment_cus_normal" />
    <fragment
        android:id="@+id/cusDimpleFragment"
        android:name="com.wdf.myrobot.ui.fragment.customize.CusDimpleFragment"
        android:label="fragment_cus_dimple"
        tools:layout="@layout/fragment_cus_dimple" />

</navigation>